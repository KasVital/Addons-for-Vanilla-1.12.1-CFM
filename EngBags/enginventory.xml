				<Scripts>
					<OnLoad>
						this:RegisterEvent("VARIABLES_LOADED");
					</OnLoad>
					<OnEvent>
						EngInventory_frame_RightClickMenu_OnLoad();
					</OnEvent>
				</Scripts>
			</Frame>
		</Frames>

		<Scripts>
			<OnLoad> 
				this:RegisterEvent("VARIABLES_LOADED");
				this:RegisterEvent("ADDON_LOADED");
			</OnLoad> 
			<OnEvent> 
				if (event == "VARIABLES_LOADED") then 
					this:RegisterEvent("BAG_UPDATE");
					this:RegisterEvent("BAG_UPDATE_COOLDOWN");
					this:RegisterEvent("ITEM_LOCK_CHANGED");
					this:RegisterEvent("UPDATE_INVENTORY_ALERTS");
					this:RegisterEvent("CRAFT_SHOW");
					this:RegisterEvent("TRADE_SKILL_SHOW");
					EngInventory_init();
				else
					EngInventory_OnEvent(event);
				end
			</OnEvent> 
			<OnMouseDown>
				EngInventory_OnMouseDown(arg1, this);
			</OnMouseDown>
			<OnMouseUp>
				EngInventory_StopMoving(this);
			</OnMouseUp>
			<OnDragStart>
				EngInventory_StartMoving(this);
			</OnDragStart>
			<OnDragStop>
				EngInventory_StopMoving(this);
			</OnDragStop>
			<OnShow>
				PlaySound("igBackPackOpen");
				EngInventory_UpdateWindow();
				EngInventory_UpdateBagState();
			</OnShow>
			<OnHide>
				PlaySound("igBackPackClose");
				EngInventory_UpdateBagState();
				EngInventory_StopMoving(this);
			</OnHide>
		</Scripts>

	</Frame> 
