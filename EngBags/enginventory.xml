		<Frames>
			<Button name="EngInventory_Button_ChangeEditMode" inherits="GameMenuButtonTemplate" text="Edit">
				<Size><AbsDimension x="85" y="25"/></Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="EngInventory_Button_HighlightToggle" relativePoint="TOPRIGHT">
						<Offset>
							<AbsDimension x="2" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						EngInventory_Button_ChangeEditMode_OnClick();
					</OnClick>
					<OnEnter>
						GameTooltip_AddNewbieTip(EILocal["EngInventory_Button_ChangeEditMode_tooltip_title"], 1.0, 1.0, 1.0, EILocal["EngInventory_Button_ChangeEditMode_tooltip"]);
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide();
					</OnLeave>
				</Scripts>
			</Button>

			<Button name="EngInventory_Button_ColumnsAdd" inherits="GameMenuButtonTemplate" text="++">
				<Size><AbsDimension x="120" y="25"/></Size>
				<Anchors>
					<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="-2" y="2"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>

					<OnClick>
						PlaySound("igMainMenuOptionCheckBoxOn");
						EngInventory_IncreaseColumns();
					</OnClick>

					<OnEnter>
						GameTooltip_AddNewbieTip(
							EILocal["EngInventory_Button_ColumnsAdd_tooltip_title"],
							1.0, 1.0, 1.0,
							EILocal["EngInventory_Button_ColumnsAdd_tooltip"] );
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide();
					</OnLeave>
				</Scripts>
			</Button>

			<Button name="EngInventory_Button_ColumnsDel" inherits="GameMenuButtonTemplate" text="--">
				<Size><AbsDimension x="120" y="25"/></Size>
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="2" y="2"/>
						</Offset>
