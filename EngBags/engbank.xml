				EngBank_OnMouseDown(arg1, this);
			</OnMouseDown>
			<OnMouseUp>
				EngBank_StopMoving(this);
			</OnMouseUp>
			<OnDragStart>
				EngBank_StartMoving(this);
			</OnDragStart>
			<OnDragStop>
				EngBank_StopMoving(this);
			</OnDragStop>
			<OnShow>
				PlaySound("igBackPackOpen");
				EngBank_UpdateWindow();
				EngBank_UpdateBagState();
			</OnShow>
			<OnHide>
