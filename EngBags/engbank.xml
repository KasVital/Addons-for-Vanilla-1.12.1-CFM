					this:RegisterEvent("PLAYER_MONEY");
		
					this:RegisterEvent("CRAFT_SHOW");
					this:RegisterEvent("TRADE_SKILL_SHOW");
				else
					EngBank_OnEvent(event);
				end
			</OnEvent> 
