	<GameTooltip name="EngBank_tt" frameStrata="TOOLTIP" hidden="true" parent="UIParent" inherits="GameTooltipTemplate">
		<Scripts>
			<OnLoad>
				this:SetOwner(UIParent, "ANCHOR_NONE");
			</OnLoad>
		</Scripts>
	</GameTooltip>

	<FontString name="EngBank_BigFont"  font="Interface\AddOns\EngBags\DAB_CooldownFont.ttf" virtual="true">
		<Shadow>
			<Offset>
				<AbsDimension x="1" y="-1"/>
			</Offset>
			<Color r="0" g="0" b="0"/>
		</Shadow>
		<FontHeight>
			<AbsValue val="16"/>
		</FontHeight>
		<Color r="1" g="0" b="0.25"/>
	</FontString>

	<Texture name="EngBank_ButtonBackgroundTemplate" file="Interface\AddOns\EngBags\skin\button_background" hidden="true" virtual="true">
		<Size>
			<AbsDimension x="64" y="64"/>
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT"/>
		</Anchors>
	</Texture>	

	<Button name="EngBank_ItemButtonTemplate" inherits="ItemButtonTemplate" hidden="true" virtual="true">
		<Size>
			<AbsDimension x="40" y="40"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Frames>
			<Model name="$parent_Cooldown" inherits="CooldownFrameTemplate"/>
		</Frames>
		<Scripts>
			<OnClick>
				EngBank_ItemButton_OnClick(arg1);
			</OnClick>
			<OnDragStart>
				EngBank_ItemButton_OnClick("LeftButton", 1);
			</OnDragStart>
			<OnReceiveDrag>
				EngBank_ItemButton_OnClick("LeftButton", 1);
			</OnReceiveDrag>
			<OnLoad>
		        this:RegisterForClicks("LeftButtonUp", "RightButtonUp");
				this:RegisterForDrag("LeftButton");
			</OnLoad>
			<OnHide>
				if ( this.hasStackSplit and (this.hasStackSplit == 1) ) then
					StackSplitFrame:Hide();
				end
			</OnHide>
			<OnEnter>
				EngBank_ItemButton_OnEnter();
			</OnEnter>
			<OnLeave>
				EngBank_ItemButton_OnLeave();
			</OnLeave>
			<OnUpdate>
				EngBank_OnUpdate(arg1);
			</OnUpdate>
		</Scripts>
	</Button>

	<Button name="EngBank_SlotTargetTemplate" inherits="ItemButtonTemplate" hidden="true" virtual="true">
		<Size>
			<AbsDimension x="40" y="40"/>
		</Size>
		<Anchors>
			<Anchor point="BOTTOMRIGHT">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="OVERLAY">
				<FontString name="$parent_BigText" inherits="EngBank_BigFont" justifyH="CENTER">
					<Anchors>
						<Anchor point="LEFT" relativePoint="LEFT">
							<Offset><AbsDimension x="-2" y="0"/></Offset>
						</Anchor>
						<Anchor point="RIGHT" relativePoint="RIGHT">
							<Offset><AbsDimension x="2" y="0"/></Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick>
				EngBank_SlotTargetButton_OnClick(arg1);
			</OnClick>
			<OnDragStart>
				EngBank_SlotTargetButton_OnClick("LeftButton", 1);
			</OnDragStart>
			<OnReceiveDrag>
				EngBank_SlotTargetButton_OnClick("LeftButton", 1);
			</OnReceiveDrag>
			<OnLoad>
		        this:RegisterForClicks("LeftButtonUp", "RightButtonUp");
				this:RegisterForDrag("LeftButton");
			</OnLoad>
			<OnHide>
				if ( this.hasStackSplit and (this.hasStackSplit == 1) ) then
					StackSplitFrame:Hide();
				end
			</OnHide>
			<OnEnter>
				EngBank_SlotTargetButton_OnEnter();
			</OnEnter>
			<OnLeave>
				EngBank_SlotTargetButton_OnLeave();
			</OnLeave>
			<OnUpdate>
				EngBank_SlotTargetButton_OnUpdate(arg1);
			</OnUpdate>
		</Scripts>
	</Button>

